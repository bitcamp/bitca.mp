.btc-background-wrapper(starfield-mouse)
  .btc-background(ng-if="!twinkle")
    canvas#starfield(starfield=""
      min-distance="22"
      sample-frequency="10"
      blink-rate="2"
    )
  .btc-background(ng-if="twinkle")
    canvas#starfield(starfield=""
      min-distance="1.5"
      sample-frequency="16"
      blink-rate="4"
    )
  #treetent
    #treetent-img
      img(
        scroll-to="#wrapper"
        src="/images/treetent.svg"
        width="100", height="100"
        ng-click="treetentClick()")

#logo.col-xs-12.noselect
  a.noselect(ui-sref="main")
    <img src="/images/logo.svg" height="150">
